        "#4\n",
        "# Read the number of test cases\n",
        "t = int(input())\n",
        "\n",
        "# Process each test case\n",
        "for _ in range(t):\n",
        "    s = input()\n",
        "    # Calculate the number of unique characters\n",
        "    unique_characters = len(set(s))\n",
        "    # Print the result\n",
        "    print(unique_characters)\n"
      ]
    }
  ]
